{
  "name": "phizog",
  "version": "0.1.0",
  "description": "Socket API Development Environment",
  "main": "main.js",
  "dependencies": {},
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.4",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.0",
    "@types/react-router": "^4.0.25",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-router-redux": "^5.0.14",
    "@types/redux-logger": "^3.0.6",
    "@types/typescript": "^2.0.0",
    "awesome-typescript-loader": "^5.0.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-devtools-installer": "^2.2.4",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.8",
    "identity-obj-proxy": "^3.0.0",
    "is-electron-renderer": "^2.0.1",
    "jest": "^23.1.0",
    "joi": "^13.3.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "photonkit": "^0.1.2",
    "prettier": "1.13.4",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.1",
    "source-map-support": "^0.5.6",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test node --trace-warnings ./test/runner.js",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 server.js",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./app/main.development",
    "dev": "npm run hot-server",
    "package": "npm run build && build --publish never",
    "package:win": "npm run build && build --win --x64",
    "package:linux": "npm run build && build --linux",
    "package:mac": "npm run build && build --mac",
    "package:all": "npm run build && build -mwl",
    "formatter": "prettier --write --config .pritterrc \"{app,test}/**/*.{js, ts, tsx}\""
  },
  "build": {
    "productName": "Phizog",
    "appId": "com.phizog.phizog",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "win": {
      "target": [
        "nsis",
        "appx"
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Development"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron",
    "webpack": "./node_modules/webpack/bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phizog/phizog.git"
  },
  "keywords": [
    "websocket",
    "socket",
    "api",
    "development",
    "environment"
  ],
  "contributors": [
    {
      "name": "Masoud Ghorbani",
      "email": "msud.ghorbani@gmail.com"
    },
    {
      "name": "Farzad Yousef Zadeh",
      "email": "farskid@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phizog/phizog/issues"
  },
  "homepage": "https://github.com/phizog/phizog#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "test-file-stub",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/preprocessor.js"
    }
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=4.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run formatter"
    }
  }
}
