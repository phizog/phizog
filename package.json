{
  "name": "phizog",
  "version": "0.1.0",
  "description": "Socket API Development Environment",
  "main": "index.js",
  "dependencies": {
    "standard": "^11.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/typescript": "^2.0.0",
    "awesome-typescript-loader": "^5.0.0",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "source-map-support": "^0.5.6",
    "style-loader": "^0.21.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test node --trace-warnings ./test/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phizog/phizog.git"
  },
  "keywords": [
    "websocket",
    "socket",
    "api",
    "development",
    "environment"
  ],
  "contributors": [
    {
      "name": "Masoud Ghorbani",
      "email": "msud.ghorbani@gmail.com"
    },
    {
      "name": "Farzad Yousef Zadeh",
      "email": "farskid@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phizog/phizog/issues"
  },
  "homepage": "https://github.com/phizog/phizog#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "test-file-stub",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ]
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=4.x"
  }
}
